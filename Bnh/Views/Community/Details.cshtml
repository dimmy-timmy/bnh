@model Bnh.Entities.Community
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="@Url.Content("~/Styles/Wall.css")" rel="stylesheet" type="text/css" />
<div class="page-view">
    <div>
        <div id="wallToolbar">
            @Html.ActionLink("Edit", "Edit", "WallTest", new { id = Model.GetWall().Id }, null)
        </div>
        @Html.Partial("WallScene", Model.GetWallBricks())
    </div>
</div>
