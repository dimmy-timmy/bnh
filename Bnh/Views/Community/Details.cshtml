@using Bnh.Entities
@model Community

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="@Url.Content("~/Styles/Wall.css")" rel="stylesheet" type="text/css" />

<div class="page-view">

<div>
    <div id="wallToolbar">
        
    </div>    
    <div id="wallScene">
    @foreach (var brick in Model.GetWallBricks())
    {
        @Html.Brick(brick)
    }
    </div>
</div>
</div>